#!/home/vishwarupa/Desktop/PRAI/praienv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'twitter==1.18.0','console_scripts','twitter-follow'
__requires__ = 'twitter==1.18.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('twitter==1.18.0', 'console_scripts', 'twitter-follow')()
    )
